pipeline {
    agent any

    stages {
        stage("Build Image") {
            agent any
            steps {
                sh "echo "Build Image""
            }
        }
        stage("Test image") {
            agent any
            steps {
                sh "echo "Test image""
            }
        }
        stage("deploy image") {
            agent any
            steps {
                sh "echo "Deploy image""
            }
        }
    }
}